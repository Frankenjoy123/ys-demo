    <sql>SELECT date&#x28;dt.date&#x29; as &#x27;date&#x27;,  ev.org_id, ev.product_base_id, pb.name as &#x27;product_name&#x27;, &#x27;&#x6240;&#x6709;&#x6279;&#x6b21;&#x27; as &#x27;key_batch_id&#x27;, count&#x28;1&#x29; as pv, count&#x28;distinct ev.user_id, ev.ys_id&#x29; as uv, count&#x28;up.product_key&#x29; as product_count from di_event ev&#xd;&#xa;inner join di_time dt on ev.time_id &#x3d; dt.id &#xd;&#xa;inner join product_base pb on pb.id &#x3d; ev.product_base_id&#xd;&#xa;left join di_user_product up on up.scan_record_id &#x3d; ev.event_id&#xd;&#xa;where  ev.name &#x3d; &#x27;scan&#x27; and date&#x28;dt.date&#x29; &#x3d; &#x27;&#x24;&#x7b;required_date&#x7d;&#x27;&#xd;&#xa;group by dt.y, dt.m, dt.d, ev.org_id, ev.product_base_id&#xd;&#xa;union all &#xd;&#xa;SELECT date&#x28;dt.date&#x29; as &#x27;date&#x27;,  ev.org_id, &#x27;All&#x27; as &#x27;product_base_id&#x27;,&#x27;&#x6240;&#x6709;&#x4ea7;&#x54c1;&#x27; as &#x27;product_name&#x27;, &#x27;&#x27; &#x27;&#x6240;&#x6709;&#x6279;&#x6b21;&#x27; as &#x27;key_batch_id&#x27;, count&#x28;1&#x29; as pv, count&#x28;distinct ev.user_id, ev.ys_id&#x29; as uv, count&#x28;up.product_key&#x29; as product_count from di_event ev&#xd;&#xa;inner join di_time dt on ev.time_id &#x3d; dt.id &#xd;&#xa;left join di_user_product up on up.scan_record_id &#x3d; ev.event_id&#xd;&#xa;where  ev.name &#x3d; &#x27;scan&#x27; and date&#x28;dt.date&#x29; &#x3d; &#x27;&#x24;&#x7b;required_date&#x7d;&#x27;&#xd;&#xa;group by dt.y, dt.m, dt.d, ev.org_id </sql>
